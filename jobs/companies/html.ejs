<div id="companies-table" class="flex flex-col">

    <%
      for(const entry of values) { 
      const route = `/company/${entry.routeid}`
      %>
      
      <div 
           data-remoteness="<%= entry['Remote status'] %>" class="border-gray-400 flex flex-row mb-2">
      <div
      class="transition duration-500 shadow ease-in-out transform hover:-translate-y-1 hover:shadow-lg select-none  bg-white dark:bg-gray-800 rounded-md w-full p-4 text-black" 
    >
          
    <div class="flex flex-1 items-center ">
      <div class="flex flex-col w-10 h-10 justify-center items-center mr-4">
        <a href="#" class="block relative">
          <img
            alt="profil"
            src="<%= entry.logo.url %>"
            class="mx-auto object-cover rounded-full h-10 w-10 "
          />
        </a>
      </div>
      <div class="flex-1 pl-1 md:mr-16">
        <div class="font-medium dark:text-white">
          <b class="text-xl"><%= entry.company %></b> 
          <a href="<%= entry.website %>" target="_blank">
            <i class="fa fa-external-link"></i>
          </a>
        </div>
        
     
    
        
        <div class="text-gray-600 dark:text-gray-200 text-sm">
          <%= entry.about %>
        </div>
        
         <div class="text-gray-600 dark:text-gray-200 text-xs">
        <span class="text-xs px-1 mr-1 font-bold leading-none text-white bg-green-600 rounded-full truncate">
        <i class="fa fa-map-marker"></i> <%= entry['Remote status'] %>
         </span>
        <span class="text-xs px-1 mr-1 font-bold leading-none text-white bg-blue-600 rounded-full truncate">
          <i class="fa fa-user"></i> <%= entry.employees %>
         </span>
        <span class="text-xs px-1 mr-1 font-bold leading-none text-white bg-purple-600 rounded-full truncate">
          <i class="fa fa-building"></i> <%= entry.type %>
         </span>
           <a href="<%= entry['twitter-url'] %>" target="_blank">
        <span class="text-xs px-1 mr-1 font-bold leading-none text-white bg-blue-300 rounded-full">
          <i class="fa fa-twitter"></i>
         </span>   
         </a>
      </div>
        
            <div class="mt-1 text-gray-600 dark:text-gray-200 text-xs">
    
        
        <%
            const val = entry['Glassdoor rating']
            const renderStar = (val, rt) => `<i class="fa ${val>=rt? "fa-star":"fa-star-o"}"></i>`;
        %>
        <% if(val) { %>
          
          
          <a href="<%= entry['Glassdoor URL'] %>" target="_blank">
            <i class="fa fa-external-link"></i>
          </a>
          
        <% let i = 1; for( i = 1; i<= 5; i++) { %>
         <%-renderStar(val, i);%>
        <% }%>
          
          
        <% } %>
       
        
      </div>
        
      </div>
      
      
    <% if(entry['positions']) { %>
        <div class="<%= entry['positions'].length >0 ? "text-gray-800" : "text-gray-400" %>">
          <a href="<%= route %>" > 
       
            
            <span class=" text-5xl" >
            <%= entry['positions'].length%>
          </span> 
          <span class="md:mr-8 mr-4"><%= entry['positions'].length == 1 ? "position" : "positions"%></span>
            
      </a>
      </div>
      <% } %>
     
      
      <a href="<%= route %>" class="md:w-8 sm:w-4 text-right flex justify-end">
        
        <svg
          width="20"
          fill="currentColor"
          height="20"
          class="hover:text-gray-800 dark:hover:text-white dark:text-gray-200 text-gray-500"
          viewBox="0 0 1792 1792"
          xmlns="http://www.w3.org/2000/svg"
        >
          <path
            d="M1363 877l-742 742q-19 19-45 19t-45-19l-166-166q-19-19-19-45t19-45l531-531-531-531q-19-19-19-45t19-45l166-166q19-19 45-19t45 19l742 742q19 19 19 45t-19 45z"
          ></path>
        </svg>
      </a>
    </div>
      
      
     <div class="grid justify-items-end">
      <div class="flex flex-1 items-center  mt-3">
    
    <% if(entry.markets) {
        for(const tag of entry.markets) { %>
        <a  href="#" >
              <span data-filterid="markets" data-value="<%= tag %>" data-allow="true" class="text-xs px-2 mr-1 font-bold leading-none text-white bg-gray-600 rounded-full truncate">
              <%= tag %>
              </span>
        </a>
          <% }} %> 
       
    
      </div>
     </div>
    </div>
    
      </div>
      
      
    <% } %>
      
    </div>
    